<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.acmed.his.dao.DiagnosisTplMapper">
    <resultMap id="diagnosisTpl" type="com.acmed.his.model.DiagnosisTpl">
        <id column="id" property="id"></id>
        <result column="orgCode" property="orgcode"></result>
        <result column="category" property="category"></result>
        <result column="diagnosis" property="diagnosis"></result>
        <result column="isValid" property="isvalid"></result>
        <result column="memo" property="memo"></result>
    </resultMap>

    <select id="getList" parameterType="java.lang.Integer" resultMap="diagnosisTpl">
        select * from t_b_diagnosis_tpl where orgcode = #{orgcode}
    </select>
</mapper>